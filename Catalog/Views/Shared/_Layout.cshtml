<!DOCTYPE html>
<html ng-app="catalog.demo" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="google-site-verification" content="wPwCjGfbYStjprMhP2nrUdJ7nHRgXsvTmOV1Duta5bI" />
    <link href='http://anthonyfassett.blogspot.com/favicon.ico' rel='icon' type='image/x-icon' />
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/font-awesome.min.css" />
    <link rel='stylesheet' type='application/x-woff' href='http://fonts.googleapis.com/css?family=Droid+Sans' />
    <link rel="stylesheet" href="~/Content/ui-grid.css" />
    <style>
        @Html.Action("Bundle", "Home", new { path = "~/Content/site.min.css", type ="text/html" })
        @RenderSection("style", required: false)
    </style>
    <title>Catalog-Demo</title>
</head>
<body ng-controller="homeCtrl">
    @Html.Partial("_Header")
    @RenderBody()
    @Html.Partial("_Footer")
    <script src="~/Scripts/angular.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.10/angular-route.min.js"></script>
    <script src="~/Scripts/ui-grid.min.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls.min.js"></script>
    <script src="~/Scripts/search.min.js"></script>
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @RenderSection("script", required: false)
</body>
</html>